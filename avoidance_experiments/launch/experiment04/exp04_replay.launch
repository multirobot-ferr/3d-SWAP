<!--

Plays the current saved rosbag

-->

<launch>

    <param name="exp_num" value="04"/>

    <!-- Prepare the bag -->
    <include file="$(find avoidance_experiments)/launch/common/rosbag_play.launch">
            <arg name="bags_path" value="$(find avoidance_experiments)/bags/experiment04"/>
            <arg name="bag_name"  value="experiment04_01.bag"/>
    </include>

</launch>
