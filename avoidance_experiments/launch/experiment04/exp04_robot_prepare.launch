<!--
Launches the swap node in robot 2
-->

<launch>
    <arg name="uav_id" default="$(env ROBOT_ID)"/>

    <!-- Swap algorithm -->
    <include file="$(find avoidance_experiments)/launch/robot/swap.launch">
        <arg name="path2goal" default="$(find avoidance_experiments)/launch/experiment04/mission4_uav$(arg uav_id).txt"/>

        <arg name="nameofbag" default="$(find avoidance_experiments)/logs/experiment04/"/>
    </include>
</launch>
