<!--
Launches the swap node in robot 2
-->

<launch>
    <arg name="uav_id" default="2"/>

    <!-- Swap algorithm -->
    <include file="$(find uav_avoidance)/launch/robot0$(arg uav_id)/swap_r$(arg uav_id).launch">
        <arg name="path2goal" default="$(find avoidance_experiments)/launch/experiment05/mission5_uav$(arg uav_id).txt"/>

        <arg name="nameofbag" default="$(find avoidance_experiments)/logs/experiment05/"/>
    </include>
</launch>
