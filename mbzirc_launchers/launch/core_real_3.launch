<?xml version="1.0"?>
<launch>

    <arg name="frames" default="$(find mbzirc_launchers)/cfg/map_karting.xml"/>
    <param name="frames_file" type="str" value="$(arg frames)" />

    <!-- Launch robot id=3 -->
    <include file="$(find px4_bringup)/launch/spawn_robot.launch">
        <arg name="id" value="3"/>
	<arg name="mode" value="3"/>
        <arg name="material" value="ZincYellow"/>
	<arg name="frames" value="$(arg frames)"/>
    </include>

</launch>
