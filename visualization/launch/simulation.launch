<launch>
    <node name="CreateFile" pkg="mbzirc_gcs_view" type="createfileworld" output="screen" args="$(find mbzirc_gcs_view)" />

    <node name="CameraView" pkg="mbzirc_gcs_view" type="cameraview" output="screen" />


		
	<include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find mbzirc_gcs_view)/world/generate_world.world"/>
    	<arg name="paused" value="true"/>
  	</include>
  	
    <include file="$(find grvc_quadrotor_hal)/launch/spawn_quad_gazebo.launch">
        <arg name="robot_name" value="quad1"/>
        <arg name="id" value="1"/>
        <arg name="pos_x" value="65"/>
        <arg name="pos_y" value="-26.5"/>
        <arg name="pos_z" value="0.3"/>
    </include>

    <include file="$(find grvc_quadrotor_hal)/launch/spawn_quad_gazebo.launch">
        <arg name="robot_name" value="quad2"/>
        <arg name="id" value="2"/>
        <arg name="pos_x" value="65"/>
        <arg name="pos_y" value="-23.5"/>
        <arg name="pos_z" value="0.3"/>
    </include>
  	
    <include file="$(find grvc_quadrotor_hal)/launch/spawn_quad_gazebo.launch">
        <arg name="robot_name" value="quad3"/>
        <arg name="id" value="3"/>
        <arg name="pos_z" value="0.3"/>
        <arg name="pos_x" value="65"/>
        <arg name="pos_y" value="-25"/>
    </include>


    <!--node name="spawn_object_blue_cylinder" pkg="gazebo_ros" type="spawn_model"
        args="-file $(find mbzirc_gcs_view)/urdf/object_blue_cylinder.sdf -sdf -x 0 -y 0 -z 1 -model object_blue_cylinder_mov" /-->

    <!--node name="MoveObject" pkg="mbzirc_gcs_view" type="moveobject" output="screen" /-->

</launch>
