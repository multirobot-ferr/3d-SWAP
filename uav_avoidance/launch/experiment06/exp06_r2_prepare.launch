<!--
Launches the swap node in robot 6
-->

<launch>
    <arg name="uav_id" default="2"/>

    <!-- Swap algorithm -->
    <include file="$(find uav_avoidance)/launch/robot0$(arg uav_id)/swap_r$(arg uav_id).launch">
        <arg name="path2goal" default="$(find uav_avoidance)/launch/experiment06/mission6_uav$(arg uav_id).txt"/>

        <arg name="nameofbag" default="$(find uav_avoidance)/logs/experiment06/"/>
    </include>
</launch>
