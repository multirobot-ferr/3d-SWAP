<!--

Launches the mission planer node
-->

<launch>

<arg name="yaw_on" default="false"/>
	<!-- Waking up all fake state machines systems -->
    <node name="auv_state_machine_1" pkg="uav_avoidance" type="delayed.sh"
		args="8 roslaunch uav_avoidance state_machine_fake.launch robot_number:=4 yaw_on:=$(arg yaw_on)" output="screen">
	</node>

</launch>
