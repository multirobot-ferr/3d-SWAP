    <!-- Swap algorithm -->
    <include file="$(find uav_avoidance)/launch/robot0$(arg uav_id)/swap_r$(arg uav_id).launch">
        <arg name="path2goal" default="$(find uav_avoidance)/launch/experiment02/mission2_uav$(arg uav_id).txt"/>
        <!-- <arg name="file_path" value="$(find uav_avoidance)/logs/experiment02/"/> -->
    </include>
</launch>
